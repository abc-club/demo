{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch node program",
      "program": "${workspaceRoot}/babel-plugin-demo/example.js",
      "runtimeExecutable": "node"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "算法",
      "program": "${workspaceRoot}/algorithms/index.js",
      "runtimeExecutable": "node"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "egg-bin",
      "cwd": "${workspaceRoot}/egg-demo",
      "program": "${workspaceRoot}/egg-demo/node_modules/egg-bin/bin/egg-bin.js",
      "args": ["dev", "${workspaceRoot}/egg-demo", "--baseDir=${workspaceRoot}/egg-demo"],
      "runtimeExecutable": "node"
    },
    {
      "name": "Launch Egg",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceRoot}/egg-demo",
      "runtimeExecutable": "npm",
      "windows": { "runtimeExecutable": "npm.cmd" },
      "runtimeArgs": ["run", "debug"],
      "console": "integratedTerminal",
      "protocol": "auto",
      "restart": true,
      "port": 9229,
      "autoAttachChildProcesses": true
    }
  ]
}
